<xmi:XMI xmi:version="20131001" xmlns:xmi="http://www.omg.org/spec/XMI/20131001" xmlns:MDMI="http://org.mdmi.uml.profile/1" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:fhir="http://www.eclipse.org/mdht/schema/profile/fhir/1.0.0" xmlns:uml="http://www.eclipse.org/uml2/5.0.0/UML">
  <uml:Package xmi:id="_H4Y3IFQIEea3DercuKCjQA" name="fhim2fhir">
    <packageImport xmi:id="_H92l8FQIEea3DercuKCjQA">
      <importedPackage href="../dstu2/FHIR-dstu2.uml#_fO7eECP_EeW3155CxDza7Q"/>
