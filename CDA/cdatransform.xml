<?xml version="1.0" encoding="UTF-8"?>
<project name="project" default="default"
	xmlns:qvto="http://www.eclipse.org/qvt/1.0.0/Operational">
	<target name="default">
		
		<delete file="map/cdamap.mdmi"/>
		<delete file="map/cdamap.xmi"/>
			
	 	 <qvto:transformation
			uri="platform:/plugin/org.mdmi.qvt.transforms/transforms/Cda2Mdmi.qvto">
			<in uri="platform:/resource/CDA/org.openhealthtools.mdht.uml.cda.consol2.model/model/consol.uml" />
	 	 	<out uri="platform:/resource/CDA/map/cdamap.mdmi" />
	 	 	<trace  uri="platform:/resource/CDA/log/cdamap.qvtotrace" ></trace>
			<configProperty name="semanticRoot" value="ClinicalDocument" />
			<configProperty name="syntaxRoot" value="ContinuityOfCareDocument" />
			<configProperty name="mappingStyle" value="NORMAL" />
		</qvto:transformation>
		
	 	 <qvto:transformation
			uri="platform:/plugin/org.mdmi.qvt.transforms/transforms/Cda2Mdmi.qvto">
			<in uri="platform:/resource/CDA/org.openhealthtools.mdht.uml.cda.consol2.model/model/consol.uml" />
	 	 	<out uri="platform:/resource/CDA/map/cdamap2.mdmi" />
	 	 	<trace  uri="platform:/resource/CDA/log/cdamap.qvtotrace" ></trace>
			<configProperty name="semanticRoot" value="ClinicalDocument2" />
			<configProperty name="syntaxRoot" value="ContinuityOfCareDocument2" />
			<configProperty name="mappingStyle" value="NORMAL" />
		</qvto:transformation>

		<transformReport reportPath="${basedir}" />
		
		<mdmiSort model="platform:/resource/CDA/map/cdamap.mdmi" />
		

	</target>
	

	
</project>